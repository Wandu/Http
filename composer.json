{
    "name": "wandu/http",
    "description": "Http Psr-7 Implementation with Session and Cookie.",
    "keywords": ["wandu", "http", "psr7", "session", "cookie"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "psr/http-message": "^1",
        "wandu/support": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Wandu\\Http\\": ""
        },
        "files" : [
            "functions.php"
        ]
    },
    "provide": {
        "psr/http-message-implementation": "1.0"
    },
    "suggest": {
        "predis/predis": "for the Redis session."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
